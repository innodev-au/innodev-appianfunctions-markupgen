<appian-plugin name="${project.name}" key="au.com.innodev.appianplugin.markupgen.plugin">

	<plugin-info>
		<description>${project.description}</description>		
		<version>${project.version}</version>
		<application-version min="17.1.0" />
		<vendor name="Innodev" url="https://www.innodev.com.au" />
	</plugin-info>
	
     <datatype key="MarkupGen_HtmlPart" name="MarkupGen Internal HTML Part">
      <class>au.com.innodev.appianplugin.markupgen.plugin.types.HtmlPart</class>
    </datatype>
    
    <datatype key="MarkupGen_XmlPart" name="MarkupGen Internal XML Part">
      <class>au.com.innodev.appianplugin.markupgen.plugin.types.XmlPart</class>
    </datatype>
    
   
	<function-category key="markupGen" name="HTML / XML Markup Generation"  />
	
    <function key="markupGen_html_newElem" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_html_newElem" />
    <function key="markupGen_html_toText" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_html_toText" />
    <function key="markupGen_html_newRawPart" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_html_newRawPart" />
    <function key="markupGen_html_newTextPart" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_html_newTextPart" />
    
    <function key="markupGen_xml_newElem" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_xml_newElem" />
    <function key="markupGen_xml_toText" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_xml_toText" />
    <function key="markupGen_xml_newRawPart" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_xml_newRawPart" />
    <function key="markupGen_xml_newTextPart" class="au.com.innodev.appianplugin.markupgen.plugin.functions.MarkupGen_xml_newTextPart" />
   
</appian-plugin>